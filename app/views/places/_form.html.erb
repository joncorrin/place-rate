<%= form_for(place) do |f| %>
<% if place.errors.any? %>
  <div id="error-explanation", class="alert alert-danger alert-dismissable">
      <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span>
      <span class="sr-only">Close</span></button>
    <h4><%= pluralize(place.errors.count, "error") %> prohibited this review from being saved:</h4>

    <ul>
    <% place.errors.full_messages.each do |message| %>
      <li><%= message %></li>
    <% end %>
    </ul>
  </div>
<% end %>

  <form>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: "form-control", id: "inputName", placeholder: "Texas Steakhouse" %>
    </div>
    <div class="form-group">
      <%= f.label :city %>
      <%= f.text_field :city, class: "form-control", id: "inputCity", placeholder: "Philadelphia" %>
    </div>
    <div class="form-group">
      <%= f.label :state %>
      <%= f.text_field :state, class: "form-control", id: "inputState", placeholder: "PA" %>
    </div>
    <div class="form-group">
      <%= f.label :website %>
      <%= f.text_field :website, class: "form-control", id: "inputWebsite", placeholder: "https://www.texassteakhouse.com" %>
    </div>
    <div class="form-group">
      <%= f.label :image %>
      <%= f.file_field :image %>
    </div>
    <div class="actions">
      <%= f.submit %>
    </div>
  </form>
<% end %>
